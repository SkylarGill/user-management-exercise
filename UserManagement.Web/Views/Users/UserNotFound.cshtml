@model UserManagement.Models.Users.UserNotFoundViewModel

@{
    ViewBag.Title = "404 - User Not Found";
}

<h1>404 - User Not Found</h1>

<p class="text-danger">No user found with ID '@Model.Id'.</p>

<button class="btn btn-outline-primary" onclick="location.href='@Url.Action("List")'">Return to Users</button>